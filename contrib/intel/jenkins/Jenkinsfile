                         --prov=verbs --util=rxm --build_hw=fire"""
                try {
                  sh """sbatch \
                        --partition=torchic \
                        -N 1 \
                        --wait \
                        -o ${output} \
                        --open-mode=append \
                        -J ${env.SLURM_JOB_NAME} \
                        --wrap=\'env; timeout 7200 ${cmd}\'
                    """
                } catch (Exception e) {
                  sh "scancel \$(cat ${output} | grep SLURM_JOBID | cut -d \"=\" -f 2)"
                  sh "cat ${output}"
                  error("Build failed ${e}")
                }
                sh "cat ${output}"
              }
            }
