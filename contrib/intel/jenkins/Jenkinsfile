          """
        }
      }
    }
    stage ('opt_out') {
      steps {
        script {
          git_diffs()
          if (weekly) {
            TIMEOUT="21600" 
          } 
          skip = skip()
          RELEASE = release()
          if (skip && !weekly) {
            DO_RUN=false
          }
        }
      }
    }
    stage ('health_check') {
      when { equals expected: true, actual: DO_RUN }
      steps {
        script {
          dir (CI_LOCATION) {
            sh "./temperature.sh"
          }
        }
      }
    }
    stage ('prepare_build') {
      when { equals expected: true, actual: DO_RUN }
