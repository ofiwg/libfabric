              dir ("${env.WORKSPACE}/${SCRIPT_LOCATION}/") {
                def providers = [["verbs", "rxm"]]
                for (mpi in MPI_TYPES) {
                  for (imb_grp = 1; imb_grp < 4; imb_grp++) {
