                def providers = [["shm", null]]
                def directions = ["h2d", "d2d", "xd2d"]
                def base_cmd = "python3.9 runtests.py --device=ze"
                def prefix = "${env.LOG_DIR}/ze_"
                def suffix = "_reg"
