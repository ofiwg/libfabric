                build_ci("pr_build_cyndaquil.json")
              }
            }
          }
        }
        stage ('quilava') {
          steps {
            script {
              dir (CI_LOCATION) {
                build_ci("pr_build_quilava.json")
              }
            }
          }
        }
        stage ('daos') {
          agent {
            node {
              label 'daos_head'
              customWorkspace CUSTOM_WORKSPACE
            }
          }
          options { skipDefaultCheckout() }
          steps {
            script {
              checkout_tar("source")
              checkout_external_resources()
